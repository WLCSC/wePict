<%= simple_form_for([@room, @bank, @question]) do |f| %>
  <%= f.error_notification %>

	<div class="form-inputs">
		<%= f.input :content %>
		<%= f.input :notes, input_html: {:rows => '4'} %>
		<%= f.input :sequence %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
